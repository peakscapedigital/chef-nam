---
// Redirect to Sanity Studio
// Production: Sanity managed hosting
// Development: Local studio on port 3333
const STUDIO_URL = import.meta.env.PROD
  ? 'https://chef-nam-catering.sanity.studio'
  : 'http://localhost:3333';

return new Response(null, {
  status: 302,
  headers: {
    Location: STUDIO_URL
  }
});
---